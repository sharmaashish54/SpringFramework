<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="mySpecification" class="com.example.autowire.constructor.Specification">
        <property name="make" value="MahindraConstructor"></property>
        <property name="model" value="XUV 700 AX5"></property>
    </bean>

    <bean id="car" class="com.example.autowire.constructor.Car" autowire="constructor"/>

</beans>